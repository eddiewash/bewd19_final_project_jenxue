<h1>Edit an event</h1>
<%= form_for @event do |f| %>
  <fieldset>
    <legend>Event</legend>
    <div>
      <%= f.label :title %>
      <%= f.text_field :title %>
    </div>
    <div>
      <%= f.label :description %>
      <%= f.text_field :description %>
    </div>
    <div>
      <%= f.label :start_time %>
      <%= f.text_field :start_time %>
    </div>
    <div>
      <%= f.label :end_time %>
      <%= f.text_field :end_time %>
    </div>
    <div>
      <%= f.label :location %>
      <%= f.text_field :location %>
    </div>

  </fieldset>
  <%= f.submit %>
<% end %>